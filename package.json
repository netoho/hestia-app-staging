{
  "name": "nextn",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "bun run next dev --turbopack -p 9002",
    "genkit:dev": "bun run genkit start -- tsx src/ai/dev.ts",
    "genkit:watch": "bun run genkit start -- tsx --watch src/ai/dev.ts",
    "build": "bun run next build",
    "start": "bun run next start",
    "lint": "bun run next lint",
    "typecheck": "bun run tsc --noEmit",
    "db:seed": "bun run prisma db seed",
    "db:setup": "./scripts/setup-database.sh",
    "db:reset": "bun run prisma db push --force-reset && bun run db:seed",
    "test": "cross-env NODE_ENV=test vitest run",
    "test:watch": "cross-env NODE_ENV=test vitest",
    "test:coverage": "cross-env NODE_ENV=test vitest run --coverage",
    "test:debug": "cross-env NODE_ENV=test vitest run --reporter=verbose",
    "test:api": "cross-env NODE_ENV=test vitest run api",
    "test:components": "cross-env NODE_ENV=test vitest run --config=vitest.components.config.ts",
    "postinstall": "bun run prisma generate --no-engine"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.4.1",
